<!-- Scaffolding generated by Casein v5.1.1.5 -->

<h2>Photos<%= casein_pagination_details @photos %></h2>
<%= form_tag edit_multiple_casein_photos_path, method: :post do %>
	<%= render :partial => 'casein/photos/table', :locals => { :photos => @photos } %>
	<%= render :partial => 'casein/layouts/multi_btns' %>
<% end %>

<div class="text-center">
	<%= will_paginate @photos %>
</div>

<%= content_for :sidebar do %>

	<li><%= link_to "#{casein_show_icon "plus-sign"}Add a new photos".html_safe, new_casein_photo_path %></li>
	Download Data: <%= link_to 'CSV', casein_photos_path(format: 'csv') %> | 
		<%= link_to 'Excel', casein_photos_path(format: 'xlsx') %>
	<br />
	<br />
	Import Data with CSV file:
	<br />
		<%= form_tag import_casein_photos_path, multipart: true do %>
			<%= file_field_tag :file, accept: 'text/csv' %>
			<%= submit_tag "Import", :class => 'btn btn-xs btn-primary pull-right' %>
		<% end %>


<% end %>
