<h2>Add a new photos</h2>

<%= form_for @photos, :url => casein_photos_path do |f| %>
	
	<%= render :partial => 'casein/photos/form', :locals => { :f => f } %>
		<%= f.fields_for :photos do |builder| %>
		<%= render 'casein/photos/photo_fields', f: builder %>
		<% end %>

	<p class="submits">
		<%= link_to 'Cancel', casein_photos_path, :class => 'btn btn-mini btn-danger' %>
		<%= f.submit "Add new photos", :class => "btn btn-mini btn-primary" %>
	</p>

<% end %>

<%= content_for :sidebar do %>
	<li><%= link_to "#{casein_show_icon "th-list"}Back to list".html_safe, casein_photos_path %></li>
<% end %>